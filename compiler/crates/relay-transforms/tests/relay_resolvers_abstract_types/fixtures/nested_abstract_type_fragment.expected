# %extensions%

interface Cat {
    description: String
}

type Tabby implements Cat {
    description: String
    parent: Cat
}

type Persian implements Cat {
    description: String
}

extend type Query {
    cat: Cat
}
==================================== OUTPUT ===================================
fragment nestedAbstractTypeFragment on Cat {
  ... on Tabby {
    parent {
      ... on Tabby {
        description
      }
      ... on Persian {
        description
      }
    }
  }
}
